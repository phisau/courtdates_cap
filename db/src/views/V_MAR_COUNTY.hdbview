view "V_MAR_COUNTY" (
    min_courtdate, max_courtdate, COUNTY_NAME, OFFENCE_name, count_per_county
) AS SELECT min(courtdate), max(courtdate), 
county_name, OFFENCE_NAME, count(*)
FROM cd_appearance
join cd_county
on cd_appearance.county_id = cd_county.id
join cd_offence
on cd_offence.id = cd_appearance.offence_id
where offence_name like '%MAR%'
GROUP BY county_name, offence_name
ORDER BY count(*) DESC 